{
	"name": "DeDuplication_SQL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Customers_Data_Dupl",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "CustomersData_DeDupl",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "RankedByOrderDate"
				},
				{
					"name": "OnlyLatestOrderDate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CustomerID as integer,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Email as string,",
				"          Country as string,",
				"          OrderDate as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 window(over(FirstName,",
				"          LastName,",
				"          Email),",
				"     desc(OrderDate, true),",
				"     rowNum = rowNumber()) ~> RankedByOrderDate",
				"RankedByOrderDate filter(rowNum == 1) ~> OnlyLatestOrderDate",
				"OnlyLatestOrderDate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['CustomersData_Unique'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}